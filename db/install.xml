<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/plugins/db" VERSION="20211217" COMMENT="XMLDB file for Moodle report/plugins"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="report_plugins" COMMENT="Details about plugins to report">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="install_path" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="title" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="developer" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="repository_url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="github_url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="plugin_url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="wiki_url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="info_url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="requested_by" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="year_added" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="category_id" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="public" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
